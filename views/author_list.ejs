
    <%- include('header') %>
    <div>
        <% if (author_list.length > 0){ %>
            <% author_list.forEach(function(author){ %>
                <li>
                    <a href="<%= author.url %>"><%= author.name %></a>
                    | <span> (<%= author.date_of_birth_formatted %> - <%= author.date_of_death_formatted %>) </span>
                </li>
            <% }); %>
        <% } else {%>
            <li>No authors</li>
        <% } %> 
    </div>
<%- include('footer') %> 